using System;

namespace Figure12
{
	class Node
	{
		private dynamic data, next;
		
		public void setData(dynamic data)
		{
			this.data = data;			
		}
		
		public dynamic getData()
		{
			return this.data;			
		}
		
		public dynamic getNext()
		{
			return this.next;			
		}
		
		public Node(dynamic data, dynamic next)
		{
			this.data = data;
			this.next = next;
		}

		public Node(dynamic data)
		{
			this.data = data;			
		}		
	}	
	
